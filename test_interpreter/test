def fibo n
    eq n 0
    if
        $ 0
        return
    endif
    eq n 1
    if
        $ 1
        return
    endif
    sub n 1
    fibo
    > r1
    sub n 2
    fibo
    > r2
    add r1 r2
    return
end

def exp a n
    eq n 1
    if
        return a
    endif
    sub n 1
    exp a
    mul a
    return
end

def fact n
    eq n 1
    if
        $ 1
        return
    endif
    sub n 1
    fact
    mul n
    return
end

exp 2.3 4
print