def fibo n
    eq n 0
    if
        $ 0
        return
    endif
    eq n 1
    if
        $ 1
        return
    endif
    sub n 1
    fibo
    > r1
    sub n 2
    fibo
    > r2
    add r1 r2
    return
end

def exp a n
    eq n 1
    if
        return a
    endif
    sub n 1
    exp a
    mul a
    return
end

def fact n
    eq n 1
    if
        $ 1
        return
    endif
    sub n 1
    fact
    mul n
    return
end

// $ "oui ceci est une string"
// > str1
// print
// arr 1 str1 4 6 "yes"
// > arr1
// print

// arr1[2]
// print

def testA fn
    add 100 0
    print
    call fn 2
    > res
    add 200 0
    print
    return res
end

def testB a
    add a 2
    print
    return
end

testA testB
print