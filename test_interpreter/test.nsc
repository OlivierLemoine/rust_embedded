def remove_first_element array
    def _rem _array n
        eq n 0
        if
            arr
            return
        endif
        sub n 1
        _rem _array
        > tmp1
        at _array n
        > tmp2
        push tmp1 tmp2
        return
    end
    len array
    > l
    sub l 1
    _rem array
    return
end

def fn1 data
    at data 0
    add 2
    > res1
    arr res1
    > res2
    
    remove_first_element data
    > data
    at data 0
    > cb

    remove_first_element data
    > res3
    concat res2 res3
    
    call cb
    
    return
end

def fn2 data
    at data 0
    mul 1.5
    return
end

arr 2 fn1 fn2
fn1
